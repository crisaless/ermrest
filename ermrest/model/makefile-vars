INSTALL_DIRS += $(PYLIBDIR)/ermrest/model

CLEAN_FILES += \
	$(ERMREST_MODEL_PYTHON_FILES:%=ermrest/model/%c) \
	$(ERMREST_MODEL_PYTHON_FILES:%=ermrest/model/%o)

EDIT_FILES += $(ERMREST_MODEL_PYTHON_FILES:%=ermrest/model/%) \
	ermrest/model/makefile-vars

